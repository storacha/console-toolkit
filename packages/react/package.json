{
  "name": "@storacha/console-toolkit-react",
  "version": "0.1.0",
  "description": "Headless React components for Storacha Console Toolkit",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --build --pretty --verbose",
    "typecheck": "tsc --build --emitDeclarationOnly --pretty --verbose",
    "dev": "tsc --build --watch --preserveWatchOutput",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "files": [
    "dist",
    "!dist/**/*.js.map"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storacha/console-toolkit.git"
  },
  "author": "Storacha Team",
  "license": "Apache-2.0 OR MIT",
  "dependencies": {
    "@ariakit/react": "^0.4.20",
    "@ariakit/react-core": "^0.4.20",
    "@storacha/did-mailto": "^1.0.2",
    "@storacha/ui-core": "^2.4.0",
    "@ucanto/client": "^9.0.2",
    "@ucanto/core": "^10.4.6",
    "@ucanto/transport": "^9.2.1",
    "ariakit-react-utils": "0.17.0-next.27",
    "multiformats": "^13.4.1"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/react": "^18.0.0",
    "fake-indexeddb": "^5.0.0",
    "happy-dom": "^12.0.0",
    "react": "^18.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  }
}